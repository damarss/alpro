Program Sekuensial1;Type Larik = array [1..100] of integer;    Procedure SequentialSearch1(A:Larik; N:integer; X:integer;  var Y:integer);    Var i:integer;    Begin	    i:=1;	    while (i<N) and (A[i] <> X) do i:= i + 1;	    if (A[i] <> X) then Y:=0	    else Y:=i;    End;    var data:larik;    n,cari,hasil:integer;Begin    n:=5;    data[1]:=31;data[2]:=22;data[3]:=17; data[4]:=19; data[5]:=26;    cari:=26;    SequentialSearch1(data,n,cari,hasil);    if hasil=0 then writeln(' Data tidak ditemukan')    else writeln('Data ditemukan pada indeks ke ',hasil);	readln;End.